{
  "env": {
    "CLAUDE_CODE_EXPERIMENTAL_AGENT_TEAMS": "1"
  },
  "permissions": {
    "allow": [
      "Read",
      "Write",
      "Edit",
      "MultiEdit",
      "Bash(git:*)",
      "Bash(dotnet:*)",
      "Bash(ls:*)",
      "Bash(cat:*)",
      "Bash(grep:*)",
      "Bash(find:*)",
      "Bash(mkdir:*)",
      "Bash(cp:*)",
      "Bash(mv:*)",
      "Bash(echo:*)",
      "Bash(touch:*)",
      "Bash(gh:*)",
      "Bash(tree:*)",
      "Bash(dir:*)",
      "Bash(rm:*)",
      "Bash(rm -f:*)",
      "Bash(rm -rf:*)",
      "Bash(rmdir:*)"
    ],
    "ask": [
      "Bash(git push:*)"
    ],
    "deny": [
      "Bash(sudo:*)",
      "Bash(su:*)"
    ]
  }
}
